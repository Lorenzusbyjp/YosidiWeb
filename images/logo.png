<svg width="200" height="200" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="200" height="200" fill="#007AFF" rx="40"/>

  <!-- Grid/CAD Lines -->
  <g stroke="#ffffff" stroke-width="3" fill="none" opacity="0.9">
    <!-- Architectural square -->
    <rect x="50" y="50" width="100" height="100" stroke-width="4"/>

    <!-- Interior lines (representing CAD grid) -->
    <line x1="100" y1="50" x2="100" y2="150"/>
    <line x1="50" y1="100" x2="150" y2="100"/>

    <!-- Corner markers -->
    <circle cx="50" cy="50" r="3" fill="#ffffff"/>
    <circle cx="150" cy="50" r="3" fill="#ffffff"/>
    <circle cx="50" cy="150" r="3" fill="#ffffff"/>
    <circle cx="150" cy="150" r="3" fill="#ffffff"/>

    <!-- Measurement lines -->
    <line x1="45" y1="50" x2="45" y2="150" stroke-width="2" opacity="0.7"/>
    <line x1="42" y1="50" x2="48" y2="50" stroke-width="2" opacity="0.7"/>
    <line x1="42" y1="150" x2="48" y2="150" stroke-width="2" opacity="0.7"/>
  </g>

  <!-- Text -->
  <text x="100" y="185" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#ffffff" text-anchor="middle">YoSiDi</text>
</svg>
