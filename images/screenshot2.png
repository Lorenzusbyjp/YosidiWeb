<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="800" height="600" fill="#f5f5f7"/>

  <!-- Device frame -->
  <rect x="50" y="30" width="700" height="540" rx="20" fill="#ffffff" stroke="#d1d1d6" stroke-width="3"/>

  <!-- CAD interface - Block library view -->
  <g>
    <!-- Toolbar -->
    <rect x="70" y="50" width="660" height="60" fill="#f9f9f9" stroke="#e5e5e7" stroke-width="1"/>
    <text x="400" y="85" font-family="Arial" font-size="18" fill="#1d1d1f" text-anchor="middle" font-weight="bold">Block Library</text>

    <!-- Block categories sidebar -->
    <rect x="70" y="120" width="150" height="430" fill="#fafafa" stroke="#e5e5e7" stroke-width="1"/>

    <!-- Category items -->
    <rect x="80" y="135" width="130" height="35" rx="5" fill="#007AFF" opacity="0.1"/>
    <text x="95" y="158" font-family="Arial" font-size="14" fill="#007AFF" font-weight="600">Furniture</text>

    <text x="95" y="198" font-family="Arial" font-size="14" fill="#666">Doors</text>
    <text x="95" y="238" font-family="Arial" font-size="14" fill="#666">Windows</text>
    <text x="95" y="278" font-family="Arial" font-size="14" fill="#666">Fixtures</text>
    <text x="95" y="318" font-family="Arial" font-size="14" fill="#666">Appliances</text>

    <!-- Blocks grid area -->
    <rect x="230" y="120" width="500" height="430" fill="#ffffff" stroke="#e5e5e7" stroke-width="1"/>

    <!-- Block items (furniture examples) -->
    <!-- Chair 1 -->
    <g transform="translate(270, 160)">
      <rect x="0" y="0" width="80" height="80" rx="5" fill="#f9f9f9" stroke="#e5e5e7" stroke-width="1"/>
      <rect x="15" y="25" width="50" height="40" rx="3" fill="none" stroke="#007AFF" stroke-width="2"/>
      <line x1="15" y1="30" x2="65" y2="30" stroke="#007AFF" stroke-width="2"/>
      <text x="40" y="100" font-family="Arial" font-size="10" fill="#666" text-anchor="middle">Chair</text>
    </g>

    <!-- Table -->
    <g transform="translate(380, 160)">
      <rect x="0" y="0" width="80" height="80" rx="5" fill="#f9f9f9" stroke="#e5e5e7" stroke-width="1"/>
      <rect x="10" y="25" width="60" height="40" rx="2" fill="none" stroke="#007AFF" stroke-width="2"/>
      <text x="40" y="100" font-family="Arial" font-size="10" fill="#666" text-anchor="middle">Table</text>
    </g>

    <!-- Desk -->
    <g transform="translate(490, 160)">
      <rect x="0" y="0" width="80" height="80" rx="5" fill="#f9f9f9" stroke="#e5e5e7" stroke-width="1"/>
      <rect x="10" y="25" width="60" height="35" rx="2" fill="none" stroke="#007AFF" stroke-width="2"/>
      <rect x="20" y="35" width="15" height="15" rx="1" fill="none" stroke="#007AFF" stroke-width="1"/>
      <text x="40" y="100" font-family="Arial" font-size="10" fill="#666" text-anchor="middle">Desk</text>
    </g>

    <!-- Sofa -->
    <g transform="translate(600, 160)">
      <rect x="0" y="0" width="80" height="80" rx="5" fill="#f9f9f9" stroke="#e5e5e7" stroke-width="1"/>
      <rect x="10" y="30" width="60" height="30" rx="3" fill="none" stroke="#007AFF" stroke-width="2"/>
      <rect x="10" y="35" width="10" height="20" rx="2" fill="none" stroke="#007AFF" stroke-width="2"/>
      <rect x="60" y="35" width="10" height="20" rx="2" fill="none" stroke="#007AFF" stroke-width="2"/>
      <text x="40" y="100" font-family="Arial" font-size="10" fill="#666" text-anchor="middle">Sofa</text>
    </g>

    <!-- Bed -->
    <g transform="translate(270, 270)">
      <rect x="0" y="0" width="80" height="80" rx="5" fill="#f9f9f9" stroke="#e5e5e7" stroke-width="1"/>
      <rect x="10" y="25" width="60" height="40" rx="2" fill="none" stroke="#007AFF" stroke-width="2"/>
      <circle cx="25" cy="35" r="5" fill="none" stroke="#007AFF" stroke-width="2"/>
      <circle cx="55" cy="35" r="5" fill="none" stroke="#007AFF" stroke-width="2"/>
      <text x="40" y="100" font-family="Arial" font-size="10" fill="#666" text-anchor="middle">Bed</text>
    </g>

    <!-- Cabinet -->
    <g transform="translate(380, 270)">
      <rect x="0" y="0" width="80" height="80" rx="5" fill="#f9f9f9" stroke="#e5e5e7" stroke-width="1"/>
      <rect x="15" y="25" width="50" height="40" rx="2" fill="none" stroke="#007AFF" stroke-width="2"/>
      <line x1="40" y1="25" x2="40" y2="65" stroke="#007AFF" stroke-width="2"/>
      <circle cx="35" cy="45" r="2" fill="#007AFF"/>
      <circle cx="45" cy="45" r="2" fill="#007AFF"/>
      <text x="40" y="100" font-family="Arial" font-size="10" fill="#666" text-anchor="middle">Cabinet</text>
    </g>
  </g>

  <!-- Placeholder text -->
  <text x="400" y="590" font-family="Arial" font-size="14" fill="#999" text-anchor="middle">Screenshot Placeholder 2 - Replace with actual app screenshot</text>
</svg>
