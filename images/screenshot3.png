<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="800" height="600" fill="#f5f5f7"/>

  <!-- Device frame -->
  <rect x="50" y="30" width="700" height="540" rx="20" fill="#ffffff" stroke="#d1d1d6" stroke-width="3"/>

  <!-- CAD interface - Measurement and dimensions view -->
  <g>
    <!-- Toolbar -->
    <rect x="70" y="50" width="660" height="60" fill="#f9f9f9" stroke="#e5e5e7" stroke-width="1"/>

    <!-- Tool icons -->
    <g transform="translate(90, 70)">
      <rect x="0" y="0" width="30" height="30" rx="5" fill="#007AFF" opacity="0.2"/>
      <text x="15" y="20" font-family="Arial" font-size="16" fill="#007AFF" text-anchor="middle" font-weight="bold">m</text>
    </g>
    <circle cx="150" cy="85" r="12" fill="#007AFF" opacity="0.1"/>
    <circle cx="190" cy="85" r="12" fill="#007AFF" opacity="0.1"/>

    <text x="650" y="85" font-family="Arial" font-size="14" fill="#007AFF" text-anchor="end" font-weight="600">Metric ▾</text>

    <!-- Drawing area with architectural plan -->
    <rect x="70" y="120" width="660" height="430" fill="#ffffff" stroke="#e5e5e7" stroke-width="1"/>

    <!-- Grid (subtle) -->
    <g stroke="#f0f0f0" stroke-width="0.5">
      <line x1="70" y1="170" x2="730" y2="170"/>
      <line x1="70" y1="220" x2="730" y2="220"/>
      <line x1="70" y1="270" x2="730" y2="270"/>
      <line x1="70" y1="320" x2="730" y2="320"/>
      <line x1="70" y1="370" x2="730" y2="370"/>
      <line x1="70" y1="420" x2="730" y2="420"/>
      <line x1="70" y1="470" x2="730" y2="470"/>
      <line x1="70" y1="520" x2="730" y2="520"/>

      <line x1="170" y1="120" x2="170" y2="550"/>
      <line x1="270" y1="120" x2="270" y2="550"/>
      <line x1="370" y1="120" x2="370" y2="550"/>
      <line x1="470" y1="120" x2="470" y2="550"/>
      <line x1="570" y1="120" x2="570" y2="550"/>
      <line x1="670" y1="120" x2="670" y2="550"/>
    </g>

    <!-- Detailed floor plan -->
    <g stroke="#1d1d1f" stroke-width="3" fill="none">
      <!-- Exterior walls -->
      <rect x="170" y="200" width="400" height="300" stroke-width="4"/>

      <!-- Interior walls -->
      <line x1="370" y1="200" x2="370" y2="500" stroke-width="4"/>
      <line x1="170" y1="350" x2="570" y2="350" stroke-width="4"/>

      <!-- Doors with arcs -->
      <path d="M 305 200 L 305 200" stroke-width="4"/>
      <path d="M 280 200 Q 280 230 305 230" stroke-width="2" stroke="#007AFF"/>

      <path d="M 305 350 Q 335 350 335 380" stroke-width="2" stroke="#007AFF"/>

      <!-- Windows -->
      <g stroke="#666" stroke-width="2">
        <line x1="420" y1="200" x2="470" y2="200"/>
        <line x1="420" y1="202" x2="470" y2="202"/>

        <line x1="570" y1="270" x2="570" y2="320"/>
        <line x1="568" y1="270" x2="568" y2="320"/>
      </g>
    </g>

    <!-- Dimensions and measurements -->
    <g stroke="#007AFF" stroke-width="1.5" fill="none">
      <!-- Horizontal dimension line (top) -->
      <line x1="170" y1="175" x2="570" y2="175"/>
      <line x1="170" y1="170" x2="170" y2="180"/>
      <line x1="570" y1="170" x2="570" y2="180"/>
      <text x="370" y="168" font-family="Arial" font-size="14" fill="#007AFF" text-anchor="middle" font-weight="600">10.50 m</text>

      <!-- Vertical dimension line (left) -->
      <line x1="145" y1="200" x2="145" y2="500"/>
      <line x1="140" y1="200" x2="150" y2="200"/>
      <line x1="140" y1="500" x2="150" y2="500"/>
      <text x="135" y="355" font-family="Arial" font-size="14" fill="#007AFF" text-anchor="middle" font-weight="600" transform="rotate(-90 135 355)">7.80 m</text>

      <!-- Interior dimension -->
      <line x1="370" y1="325" x2="570" y2="325"/>
      <line x1="370" y1="322" x2="370" y2="328"/>
      <line x1="570" y1="322" x2="570" y2="328"/>
      <text x="470" y="318" font-family="Arial" font-size="12" fill="#007AFF" text-anchor="middle">5.20 m</text>
    </g>

    <!-- Area labels -->
    <g>
      <!-- Room 1 area -->
      <rect x="215" y="260" width="90" height="35" rx="5" fill="#007AFF" opacity="0.1" stroke="#007AFF" stroke-width="1.5"/>
      <text x="260" y="275" font-family="Arial" font-size="11" fill="#007AFF" text-anchor="middle">Living Room</text>
      <text x="260" y="290" font-family="Arial" font-size="14" fill="#007AFF" text-anchor="middle" font-weight="bold">40.95 m²</text>

      <!-- Room 2 area -->
      <rect x="420" y="260" width="90" height="35" rx="5" fill="#00C853" opacity="0.1" stroke="#00C853" stroke-width="1.5"/>
      <text x="465" y="275" font-family="Arial" font-size="11" fill="#00C853" text-anchor="middle">Kitchen</text>
      <text x="465" y="290" font-family="Arial" font-size="14" fill="#00C853" text-anchor="middle" font-weight="bold">22.36 m²</text>

      <!-- Room 3 area -->
      <rect x="215" y="410" width="90" height="35" rx="5" fill="#FF6F00" opacity="0.1" stroke="#FF6F00" stroke-width="1.5"/>
      <text x="260" y="425" font-family="Arial" font-size="11" fill="#FF6F00" text-anchor="middle">Bedroom</text>
      <text x="260" y="440" font-family="Arial" font-size="14" fill="#FF6F00" text-anchor="middle" font-weight="bold">28.47 m²</text>
    </g>

    <!-- Measurement points -->
    <circle cx="170" cy="200" r="4" fill="#007AFF"/>
    <circle cx="570" cy="200" r="4" fill="#007AFF"/>
    <circle cx="570" cy="500" r="4" fill="#007AFF"/>
    <circle cx="170" cy="500" r="4" fill="#007AFF"/>
  </g>

  <!-- Placeholder text -->
  <text x="400" y="590" font-family="Arial" font-size="14" fill="#999" text-anchor="middle">Screenshot Placeholder 3 - Replace with actual app screenshot</text>
</svg>
