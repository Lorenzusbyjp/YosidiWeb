<svg width="64" height="64" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="64" height="64" fill="#007AFF" rx="12"/>

  <!-- Simple CAD icon -->
  <g stroke="#ffffff" stroke-width="2.5" fill="none">
    <!-- Main square (building outline) -->
    <rect x="16" y="16" width="32" height="32" stroke-width="3"/>

    <!-- Cross lines (CAD grid) -->
    <line x1="32" y1="16" x2="32" y2="48"/>
    <line x1="16" y1="32" x2="48" y2="32"/>

    <!-- Corner markers -->
    <circle cx="16" cy="16" r="1.5" fill="#ffffff"/>
    <circle cx="48" cy="16" r="1.5" fill="#ffffff"/>
    <circle cx="16" cy="48" r="1.5" fill="#ffffff"/>
    <circle cx="48" cy="48" r="1.5" fill="#ffffff"/>
  </g>
</svg>
